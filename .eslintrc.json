{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["prettier", "plugin:prettier/recommended"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true,
        "parser": "flow"
      }
    ]
    // "no-unused-vars": [2, {"vars": "all", "args": "after-used"}]
    // "singleQuote": true,
    // "no-console": "warn"
  }
}
